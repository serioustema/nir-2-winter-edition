{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icon-128.png",
         "16": "icon-16.png",
         "48": "icon-48.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "__MSG_app_name__"
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "ru",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "manifest_version": 2,
   "name": " Audio NIR 2: Winter Edition",
   "short_name" : "Audio 2",
	"version" : "2.0",
	"author" : "Lobanoff",
  "description" : "Второй тестовый пример работы расширения для Google Chrome в виде плагина перехвата и усиления или уменьшения громкости звука. By Lobanoff Artem 2021",
   "permissions": [ "tabCapture", "tabs", "storage", "\u003Call_urls>" ],
   "web_accessible_resources": [ "*.css" ]
}
